<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>

<FlowPane
        padding="20"
        xmlns:fx="http://javafx.com/fxml/1" prefHeight="250.0" prefWidth="300.0"
        fx:controller="com.ua.robot.lesson4.Main.Controller">
    <TextField fx:id="text"/>
    <Button fx:id="button" text="Обробити символьні рядки!" onAction="#handleButtonPress"/>
    <Label fx:id="label" visible="false"/>
</FlowPane>


